sleep 0.5
SHO="PSKINdQe1GyxGgecYz2191H2JoS9qvgD"
{
shodan init "$SHO"
} &> /dev/null
fi
SAS="$2"
SIS="$3"
shodan download "$SAS" "$SIS"
exit

