{
shodan init "$SHO"
} &> /dev/null
fi
copycat-map
exit

elif [[ "$1" = "update" ]]
then
echo ""$BS"Installing update..."$CE""
{
rm -r ~/copycat
rm /usr/local/bin/copycat
rm /usr/local/bin/copycat-map
cd ~
git clone https://github.com/entynetproject/copycat.git
cd copycat
chmod +x install.sh
./install.sh
} &> /dev/null

elif [[ "$1" = "info" ]]
then
echo
cd ~/copycat
cat banner/banner1.txt        
echo  
echo  "   Name     : Copycat Toolkit"
echo  "   Author   : Entynetproject (Ivan Nikolsky, Anna Fet)"
echo  "   Version  : v1.0 (first release)"
echo  "   Database : Shodan (https://shodan.io/)"
echo  "   Site     : https://entynetproject.simplesite.com/"
